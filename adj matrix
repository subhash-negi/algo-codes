#include <iostream>
#include <vector>
using namespace std;
int matrix[5][5];
void addedge(int u,int v)
{
    matrix[u][v]=1;
    matrix[v][u]=1;
}
void print()
{
    for(int i=0;i<5;i++)
    {
        cout<<"\n";
        for(int j=0;j<5;j++)
        {
            cout<<matrix[i][j]<<" ";
        }
    }
}
int main()
{
    addedge(0,1);
    addedge(0,4);
    addedge(1,2);
    addedge(1,3);
    addedge(1,4);
    addedge(4,3);
    addedge(3,2);
    print();
}
